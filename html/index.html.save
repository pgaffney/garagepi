   <title>Garage Door Opener</title>
   <script type="text/javascript" src="/webiopi.js"></script>
   <script type="text/javascript">
        webiopi().ready(function() {
            var content, button;
            content = $("#content");

            // we're using GPIO4 (bcm numbering)
            var DOOR1 = 4;
            var DOOR2 = 17;

            // make sure the GPIO is set to OUT in case it was changed from initial startup
		    webiopi().setFunction(DOOR1, "OUT");
		    webiopi().setFunction(DOOR2, "OUT");

            // create a button which output a bit sequence on GPIO 7 with a 100ms period 
            button = webiopi().createSequenceButton("clickButton", "Garage Door 1", DOOR1, 1000, '10');
            content.append(button); // append button to content div

            // create a button which output a bit sequence on GPIO 7 with a 100ms period 
            button = webiopi().createSequenceButton("clickButton", "Garage Door 2", DOOR2, 1000, '10');
            content.append(button); // append button to content div

            // auto refresh UI
            webiopi().refreshGPIO(true);

        });

    </script>
    <style type="text/css">
    </style>
</head>
<body id="background">
    <div id="content" align="center"></div>
</body>




